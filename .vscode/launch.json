{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run script.gds",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/cli/bin/Debug/net10.0/gdialog.dll",
            "args": ["script.gds"],
            "cwd": "${workspaceFolder}/cli",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Run --help",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/cli/bin/Debug/net10.0/gdialog.dll",
            "args": ["--help"],
            "cwd": "${workspaceFolder}/cli",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Run --about",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/cli/bin/Debug/net10.0/gdialog.dll",
            "args": ["--about"],
            "cwd": "${workspaceFolder}/cli",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Docs: Start",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/docs",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["start"],
            "console": "integratedTerminal"
        }
    ]
}
